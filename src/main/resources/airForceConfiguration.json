{
  "server":{
    "port":9090,
    "static":{
      "path" : "static",
      "root" : "webroot",
      "encoding" : "UTF-8",
      "cache":{
        "enable":true,
        "maxAge":86400,
        "cacheTimeout": 30000,
        "maxCacheSize":10000
      }
    }
  },
  "cloud":{
    "service":{
      "name":"my-app",
      "register":true,
      "fetch-registry":false
    },
    "zookeeper":{
      "namespace":"airForce",
      "servers":["127.0.0.1:2181"],
      "sessionTimeoutMs": 60000,
      "connectionTimeoutMs":15000,
      "retry":{
        "intervalMs": 2000
      }
    }
  }

}